import pandas as pd
import numpy as np
import datetime

data = pd.read_csv("./us_stocks.csv")

data.date = pd.to_datetime(data.date, format="%Y-%m-%d")
data = data[data.date >= datetime.datetime(2005, 12, 31)].copy()
data.index = [data.date, data.ticker]
data = data[data.columns[-5:]].copy()
col_names = ["open", "high", "low", "close", "volume"]
data.columns = col_names

data.to_csv("./us_stocks_new.csv")
